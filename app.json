{
	"--envs": [
		"//localhost/index/",
		"http://index.local/",
		"https://demo.jotunn.org/index/",
		"https://gitcdn.xyz/repo/JOTUNNDigital/index-cdn/master/"
	],

	"repository": "//localhost/index/",
	"filesystem": {
		"base": "//localhost/index/",

		"assets": "app/assets",
		"content": "app/content",
		"vendor": "app/vendor"
	},
	"features": [
		"swipe",
		"net",
		"dialog"
	],
	"routes": [
		{
			"path": "/",
			"href": "[@filesystem.content]/"
		},
		{
			"path": "/test",
			"href": "[@filesystem.content]/test.html"
		}
	],
	"settings": {
		"transitions": {
			"enabled": true,
			"duration": 250
		}
	}
}